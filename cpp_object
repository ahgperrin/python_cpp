#include <pybind11/pybind11.h>
struct Plane {
Plane(const std::string &name) : name(name) { }
void setName(const std::string &name_) { name = name_; } 
const std::string &getName() const { return name; }
    std::string name;
};


namespace py = pybind11;
PYBIND11_MODULE(object, obj) { py::class_<Plane>(obj, "Plane")
}
.def(py::init<const std::string &>()) .def("setName", &Plane::setName) .def("getName", &Plane::getName);
